<div class="container">
  <div class="row mt-4">
    <div class="col-10 mx-auto">
      <h2 class="text-center w-25 h4 bg-light">商品新規登録</h2>
    </div>
  </div>

  <div class="row mt-4">
    <div class="col-6 mx-auto">
      <%= render 'layouts/errors', obj: @item %>
      <%= form_with model: @item, url: admin_items_path, method: :post do |f| %>
        <%= render :partial => 'form', :locals => {:f => f} %>
      <% end %>
    </div>
  </div>
</div>